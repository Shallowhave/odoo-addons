<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- 权限规则 -->
    <record id="mrp_production_return_wizard_rule" model="ir.rule">
        <field name="name">制造订单返回向导规则</field>
        <field name="model_id" ref="model_mrp_production_return_wizard"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
    </record>
    
    <record id="mrp_production_return_history_rule" model="ir.rule">
        <field name="name">制造订单返回历史规则</field>
        <field name="model_id" ref="model_mrp_production_return_history"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
    </record>
    
    <record id="mrp_return_reason_rule" model="ir.rule">
        <field name="name">返回原因规则</field>
        <field name="model_id" ref="model_mrp_return_reason"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>
