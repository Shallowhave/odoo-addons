<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="xq_rfid.extended_mrp_quality_check_confirmation_dialog"
              t-inherit="mrp_workorder.MrpQualityCheckConfirmationDialog"  t-inherit-mode="extension" owl="1">
        <xpath expr="//t/div/Dialog" position="inside">
            <!-- RFID 标签信息显示组件 -->
            <div t-if="recordData.test_type === 'rfid_label'" class="workorder_rfid_generation">
                <RfidGenerationWizard t-props="rfidInfo"/>
            </div>
        </xpath>
    </t>
</templates>
